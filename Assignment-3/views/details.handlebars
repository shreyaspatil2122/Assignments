{{> header}}
<h1 class="text-center mt-3">Details {{name}}</h1>
<div class="container">
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Age</th>
            <th scope="col">Phone</th>
            <th scope="col">City</th>
        </tr>
    </thead>
    {{#each info}}
    <tbody>
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.age}}</td>
            <td>{{this.phone}}</td>
            <td>{{this.city}}</td>
        </tr>
    </tbody>
    {{/each}}
</table>
</div>
{{> footer}}